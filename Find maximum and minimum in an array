using System;
class Program21
{
    static void Main()
    {
        Console.Write("Enter the number of elements: ");
        int n = int.Parse(Console.ReadLine());
        int[] arr = new int[n];
        Console.WriteLine("Enter the elements:");
        for (int i = 0; i < n; i++)
        {
            arr[i] = int.Parse(Console.ReadLine());
        }
        int max = arr.Max();
        int min = arr.Min();
        Console.WriteLine($"Maximum: {max}, Minimum: {min}");
    }
}
